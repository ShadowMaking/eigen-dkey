{
    "name": "eigen-dkey",
    "version": "1.0",
    "description": "A non-custodial, efficient and refreshable key management service",
    "icons": {
      "16": "images/app.png",
      "48": "images/app.png",
      "128": "images/app.png"
    },
    "manifest_version": 3,
    "page_action": {
      "default_icon": "images/icon.png",
      "default_title": "eigen_dkey",
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ],
        "run_at": "document_end"
      }
    ],
    "permissions": [
        "contextMenus", 
		"tabs", 
		"notifications", 
		"webRequest", 
		"webRequestBlocking", 
		"storage", 
		"http://*/*", 
		"https://*/*"
    ],
    "web_accessible_resources": [
        {
          "resources": [
            "/images/app.png"
          ],
          "matches": [
            "<all_urls>"
          ]
        }
      ],
      "content_security_policy": {

      },
      "action": {
        "default_popup": "index.html",
        "default_icon": {
          "16": "/images/app.png",
          "32": "/images/app.png",
          "48": "/images/app.png",
          "128": "/images/app.png"
        },
        "icons": {
          "16": "/images/app.png",
          "32": "/images/app.png",
          "48": "/images/app.png",
          "128": "/images/app.png"
        }
      }
  }